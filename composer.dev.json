{
  "name": "hershel-theodore-layton/static-type-assertion-code-generator",
  "description": "Codegen functions equivalent to type testing `as` expressions.",
  "license": "MIT",
  "require": {
    "hhvm": "^4.102",
    "hershel-theodore-layton/type-visitor": "^1"
  },
  "require-dev": {
    "hershel-theodore-layton/test-chain": "<1",
    "hershel-theodore-layton/expect": "<1",
    "hershel-theodore-layton/portable-hack-ast-linters-server": "<1"
  },
  "scripts": {
    "post-install-cmd": [".github/workflows/prepare-environment.sh"],
    "post-update-cmd": [".github/workflows/prepare-environment.sh"]
  },
  "config": {
    "allow-plugins": {
      "hhvm/hhvm-autoload": true
    }
  }
}
